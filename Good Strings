import java.util.*;
public class Main {
public static int goodString(String s1, String s2) {
int n = s1.length();
int m = s2.length();
for(int i=0; i<Math.min(n,m); i++) {
value = value+Math.abs((int)s2.charAt(j)-(int)s1.charAt(n-m+i));
}
return value;
}
public static void main(String args[]) {
Scanner sc = new Scanner(System.in);
String s1 = sc.nextLine();
String s2 = sc.nextLine();
System.out.println(goodString(s1,s2));
}
}
